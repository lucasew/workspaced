# Workspaced Configuration
# Generated by: workspaced self-install
# Hostname: {{ .Hostname }}

[desktop]
dark_mode = true

[hosts.{{ .Hostname }}]
ips = {{ .LocalIPs | toJSON }}
port = 22

[browser]
default = "firefox"
webapp = "chromium"

# Enable example module to see how workspaced works
[modules.example]
enable = true
